<h2>WHY DRAFT PICKS</h2>

<div class="container">
<h5>pick here</h5>
</div>



<div class="panel panel-default">
    <div class="panel-heading">Panel heading without title</div>
    <div class="panel-body">
        <div class="table-responsive">
            <table class="table table-condensed">
                <thead>
                <tr>
                    <th>Bachelor Contestant Name</th>
                    <th>Age</th>
                    <th>Occupation</th>
                    <th>Description</th>
                </tr>
                </thead>
                <tbody>
                {{#each data}}
                    <tr>
                        <td>{{this.name}}</td>
                        <td>{{this.occupation}}</td>
                        <td>{{this.age}}</td>
                        <td>{{this.description}}</td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
