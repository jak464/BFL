
    <div class="container">
        <div class="table-responsive">
            <table class="table table-condensed">
                <thead>
                <tr>
                    <th>Id</th>
                    <th>League Name</th>
                    <th>League Owner</th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                {{#each data}}
                    <tr>
                        <td><a href="/leagueDetailView/{{this.id}}">{{this.id}}</a></td>
                        <td>{{this.leagueName}}</td>
                        <td>{{this.leagueOwner}}</td>
                        </td>
                        <td>
                            <a href="/joinLeagueView/{{this.id}}">Join League</a>
                        </td>
                    </tr>
                {{/each}}
                </tbody>
            </table>
        </div>
    </div>
